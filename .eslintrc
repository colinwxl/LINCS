{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "globals": {
    "Action": false,
    "Image": false,
    "__DEV__": false,
    "__PROD__": false,
    "__DEBUG__": false,
    "__DEBUG_NEW_WINDOW__": false,
    "__BASENAME__": false,
  },
  "rules": {
    "import/no-unresolved": [0, {}],
  },
  "settings" : {
    "import/resolver": {
      "webpack": {
        "config": "./build/webpack.config.js"
      }
    }
  },
}
