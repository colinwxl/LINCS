{
  "name": "lincs",
  "version": "1.6.3",
  "description": "A home for the LINCS project.",
  "main": "src/main.js",
  "engines": {
    "node": ">=4.2.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "preversion": "docker-machine start default",
    "version": "npm run deploy:docker && git add -A .",
    "postversion": "git push origin master && git push origin master --tags && npm run clean",
    "deploy:docker": "npm run server-es5 && npm run build:prod && npm version | tee build.log || exit 1 && VERSION=$(head -1 build.log | awk '{print substr($3, 2, length($3) - 3);}') && docker build -t 146.203.54.165:5000/lincs:$VERSION . && docker push 146.203.54.165:5000/lincs:$VERSION && curl -H \"Content-Type: application/json\" -X PUT -d '{ \"instances\": 1, \"cpus\": 1, \"mem\": 2048, \"env\": { \"NODE_ENV\": \"production\" }, \"container\": { \"type\": \"DOCKER\", \"docker\": { \"image\": \"146.203.54.165:5000/lincs:'\"$VERSION\"'\", \"forcePullImage\": false, \"network\": \"BRIDGE\", \"portMappings\": [ { \"containerPort\": 3000, \"hostPort\": 0, \"protocol\": \"tcp\" } ] }, \"volumes\": [ {\"containerPath\": \"/usr/src/dist/files/datasets\", \"hostPath\": \"/hadoop/hdfs-mount/apps/lincs/datasets\", \"mode\": \"RO\"} ] }, \"healthChecks\": [ { \"protocol\": \"HTTP\", \"path\": \"/LINCS/api/v1/health\", \"gracePeriodSeconds\": 10, \"intervalSeconds\": 60, \"portIndex\": 0, \"timeoutSeconds\": 10, \"maxConsecutiveFailures\": 3 }], \"labels\": { \"public\": \"true\" } }' maayanlab:systemsbiology@146.203.54.165:8080/v2/apps/lincs && rm build.log",
    "clean": "rm -rf dist server-es5",
    "compile": "babel-node bin/compile",
    "build:prod": "better-npm-run build:prod",
    "dev": "better-npm-run dev",
    "dev:nw": "npm run dev -- --nw",
    "dev:no-debug": "npm run dev -- --no_debug",
    "elastic": "better-npm-run elastic",
    "server-es5": "rm -rf server-es5 && babel server --out-dir server-es5",
    "lint": "eslint . ./",
    "lint:fix": "npm run lint -- --fix",
    "migrate": "better-npm-run migrate",
    "migrate:prod": "better-npm-run migrate:prod",
    "migrate:omit-data": "better-npm-run migrate --omit-data",
    "migrate:prod:omit-data": "better-npm-run migrate:prod --omit-data",
    "seed": "better-npm-run seed",
    "seed:prod": "better-npm-run seed:prod",
    "seed:omit-data": "better-npm-run seed:omit-data",
    "seed:prod:omit-data": "better-npm-run seed:prod:omit-data",
    "start": "better-npm-run start",
    "start:prod": "better-npm-run start:prod",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch"
  },
  "betterScripts": {
    "dev": {
      "command": "nodemon --exec babel-node server/main",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "build:prod": {
      "command": "npm run clean && npm run compile",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "elastic": {
      "command": "babel-node server/data/elastic",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "migrate": {
      "command": "babel-node server/data/migrate",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "migrate:prod": {
      "command": "babel-node server/data/migrate",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "seed": {
      "command": "npm run server-es5 && node --max-old-space-size=15360 server-es5/data/seed && rm -rf server-es5",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "seed:prod": {
      "command": "npm run server-es5 && node --max-old-space-size=15360 server-es5/data/seed && rm -rf server-es5",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "seed:omit-data": {
      "command": "npm run server-es5 && node --max-old-space-size=15360 server-es5/data/seed --omit-data && rm -rf server-es5",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "seed:prod:omit-data": {
      "command": "npm run server-es5 && node --max-old-space-size=15360 server-es5/data/seed --omit-data && rm -rf server-es5",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "babel-node bin/server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "start:prod": {
      "command": "node dist/main",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "babel-node ./node_modules/karma/bin/karma start bin/karma",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mgmcdermott/L1000.git"
  },
  "author": "Michael McDermott <michael@mgmcdermott.com> (http://mgmcdermott.com)",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.9.1",
    "babel-runtime": "^6.6.1",
    "better-npm-run": "0.0.8",
    "bookshelf": "^0.9.4",
    "boom": "^3.1.2",
    "bootstrap": "^4.0.0-alpha.2",
    "camelize": "^1.0.0",
    "crypto": "0.0.3",
    "d3": "^3.5.17",
    "debug": "^2.2.0",
    "elasticsearch": "^11.0.1",
    "es6-promise": "^3.1.2",
    "eslint": "^2.8.0",
    "extend": "^3.0.0",
    "history": "^2.0.2",
    "html-webpack-plugin": "^2.16.0",
    "jquery": "^2.2.2",
    "jquery-ui": "^1.10.5",
    "jsonwebtoken": "^5.7.0",
    "knex": "^0.10.0",
    "koa": "^2.0.0-alpha.3",
    "koa-bodyparser": "^2.0.1",
    "koa-compress": "^1.0.8",
    "koa-connect-history-api-fallback": "^0.3.0",
    "koa-convert": "^1.2.0",
    "koa-cors": "0.0.16",
    "koa-jwt": "^1.1.2",
    "koa-logger": "^1.3.0",
    "koa-mount": "^2.0.0",
    "koa-router": "^7.0.1",
    "koa-sendfile": "^2.0.0",
    "koa-static": "^2.0.0",
    "lodash": "^4.11.1",
    "moment": "^2.13.0",
    "mysql": "^2.10.2",
    "nodemailer": "^2.3.2",
    "normalizr": "^2.0.0",
    "react": "^15.0.1",
    "react-addons-css-transition-group": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-modal": "^1.1.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.2.4",
    "react-router-redux": "^4.0.0",
    "redux": "^3.3.1",
    "redux-form": "^5.0.1",
    "redux-thunk": "^2.0.1",
    "tether": "^1.2.2",
    "twit": "^2.2.3",
    "url": "^0.11.0",
    "webpack": "^1.13.0",
    "whatwg-fetch": "^0.11.0",
    "yargs": "^4.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.3.17",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-react-constant-elements": "^6.5.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.3",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "css-loader": "^0.23.0",
    "cssnano": "^3.3.2",
    "es6-promise": "^3.1.2",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-jsx-a11y": "^0.6.2",
    "eslint-plugin-react": "^4.3.0",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.0",
    "file-loader": "^0.8.4",
    "fs-extra": "^0.26.3",
    "html-webpack-plugin": "^2.12.0",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "jwt-decode": "^2.0.1",
    "karma": "^0.13.8",
    "karma-coverage": "^0.5.0",
    "karma-mocha": "^0.2.0",
    "karma-phantomjs-launcher": "^1.0.0",
