{
  "name": "LINCS",
  "version": "1.0.0",
  "description": "A home for the LINCS project.",
  "main": "src/main.js",
  "engines": {
    "node": ">=4.2.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "build:docker": "npm run server-es5 && npm run deploy && docker build -t 146.203.54.165:5000/lincs . && rm -rf server-es5",
    "clean": "rm -rf dist",
    "compile": "babel-node bin/compile",
    "deploy": "better-npm-run deploy",
    "dev": "better-npm-run dev",
    "dev:nw": "npm run dev -- --nw",
    "dev:no-debug": "npm run dev -- --no_debug",
    "server-es5": "rm -rf server-es5 && babel server --out-dir server-es5",
    "flow:check": "babel-node bin/flow-check",
    "lint": "eslint . ./",
    "lint:fix": "npm run lint -- --fix",
    "migrate": "better-npm-run migrate",
    "push:docker": "docker push 146.203.54.165:5000/lincs",
    "seed": "better-npm-run seed",
    "start": "better-npm-run start",
    "start:prod": "better-npm-run start:prod",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch"
  },
  "betterScripts": {
    "dev": {
      "command": "nodemon --exec babel-node server/main",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "migrate": {
      "command": "babel-node server/data/migrate",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "seed": {
      "command": "npm run server-es5 && node --max-old-space-size=15360 server-es5/data/seed && rm -rf server-es5",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "seed:prod": {
      "command": "npm run server-es5 && node --max-old-space-size=15360 server-es5/data/seed && rm -rf server-es5",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "babel-node bin/server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "start:prod": {
      "command": "node dist/server",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test": {
      "command": "babel-node ./node_modules/karma/bin/karma start bin/karma",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mgmcdermott/L1000.git"
  },
  "author": "Michael McDermott <michael@mgmcdermott.com> (http://mgmcdermott.com)",
  "license": "MIT",
