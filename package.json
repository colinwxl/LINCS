    "react-addons-test-utils": "^15.0.1",
    "react-hot-loader": "^3.0.0-alpha.12",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.2",
    "redbox-react": "^1.2.2",
    "sass-loader": "^3.0.0",
    "source-map-support": "^0.4.0",
